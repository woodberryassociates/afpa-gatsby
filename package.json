{
	"name": "afpa-wordpress",
	"private": false,
	"description": "AfPA's WordPress website with Gatsby",
	"version": "1.0.0",
	"author": "Avi Nerenberg <avinerenberg@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/avinoamsn/afpa-gatsby"
	},
	"bugs": {
		"url": "https://github.com/avinoamsn/afpa-gatsby/issues"
	},
	"keywords": [
		"gatsby",
		"AfPA",
		"Alliance",
		"Patient",
		"Access"
	],
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version"
		]
	},
	"scripts": {
		"prestart": "npm run build:lambda",
		"start": "run-p start:gatsby start:lambda",
		"start:gatsby": "gatsby develop",
		"start:lambda": "netlify-lambda serve src/lambda",
		"build": "run-s build:gatsby build:lambda",
		"build:gatsby": "gatsby build",
		"build:lambda": "netlify-lambda build src/lambda",
		"serve": "run-p serve:gatsby serve:lambda",
		"serve:gatsby": "gatsby serve",
		"serve:lambda": "netlify-lambda serve src/lambda -p 9001",
		"clean": "gatsby clean",
		"lint": "eslint src --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint --fix src --ext .js,.jsx,.ts,.tsx"
	},
	"babel": {
		"plugins": [
			[
				"@babel/plugin-proposal-decorators",
				{
					"legacy": true
				},
				"add-react-displayname"
			]
		],
		"presets": [
			[
				"babel-preset-gatsby",
				{
					"targets": {
						"browsers": [
							">0.25%",
							"not dead"
						]
					}
				}
			]
		]
	},
	"eslintConfig": {
		"extends": "react-app",
		"rules": {
			"quotes": [
				1,
				"backtick"
			]
		}
	},
	"prettier": {
		"trailingComma": "es5",
		"useTabs": true,
		"semi": false,
		"singleQuote": true
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "npx --no-install commitlint --edit $1"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.3.0",
		"@types/react-helmet": "^5.0.15",
		"autoprefixer": "^9.7.4",
		"babel-plugin-add-react-displayname": "^0.0.5",
		"elasticlunr": "^0.9.5",
		"gatsby": "^2.18.22",
		"gatsby-background-image": "^0.8.19",
		"gatsby-cli": "^2.12.10",
		"gatsby-gravityforms-component": "^2.0.1",
		"gatsby-image": "^2.2.38",
		"gatsby-plugin-facebook-pixel": "^1.0.3",
		"gatsby-plugin-google-gtag": "^2.1.1",
		"gatsby-plugin-manifest": "^2.2.37",
		"gatsby-plugin-postcss": "^2.1.19",
		"gatsby-plugin-react-helmet": "^3.1.21",
		"gatsby-plugin-remove-serviceworker": "^1.0.0",
		"gatsby-plugin-sharp": "^2.3.13",
		"gatsby-plugin-typescript": "^2.1.26",
		"gatsby-source-filesystem": "^2.1.46",
		"gatsby-source-gravityforms": "^1.0.15",
		"gatsby-source-wordpress": "^4.0.8",
		"gatsby-transformer-sharp": "^2.3.12",
		"he": "^1.2.0",
		"lint-staged": "^10.1.3",
		"logrocket": "^1.2.3",
		"logrocket-react": "^4.0.1",
		"netlify-lambda": "^1.6.3",
		"npm-run-all": "^4.1.5",
		"postcss-easy-import": "^3.0.0",
		"postcss-preset-env": "^6.7.0",
		"prop-types": "^15.7.2",
		"pure-react-carousel": "^1.24.1",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^5.2.1",
		"react-media-player": "^0.7.9",
		"react-player": "^1.14.2",
		"react-twitter-widgets": "^1.7.1",
		"rss-parser": "^3.7.3",
		"smooth-scroll": "^16.1.1",
		"sugarss": "^2.0.0",
		"typeface-montserrat": "0.0.75"
	},
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "^7.14.2",
		"@commitlint/cli": "^12.0.1",
		"@commitlint/config-conventional": "^12.0.1",
		"@types/node": "^12.12.24",
		"@types/react": "^16.9.17",
		"@types/react-dom": "^16.9.4",
		"@typescript-eslint/eslint-plugin": "^2.17.0",
		"@typescript-eslint/parser": "^2.17.0",
		"babel-eslint": "^10.0.3",
		"babel-preset-gatsby": "^1.6.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.9.0",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.18.0",
		"husky": "^4.2.1",
		"postcss-easing-gradients": "^3.0.1",
		"precss": "^4.0.0",
		"prettier": "^1.19.1",
		"tailwindcss": "^1.1.4",
		"typescript": "^3.7.4"
	}
}
